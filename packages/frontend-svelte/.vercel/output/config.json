{
	"version": 3,
	"routes": [
		{
			"src": "/_app/immutable/.+",
			"headers": {
				"cache-control": "public, immutable, max-age=31536000"
			}
		},
		{
			"handle": "filesystem"
		},
		{
			"src": "^/?(?:/__data.json)?$",
			"dest": "/"
		},
		{
			"src": "^/api/ai/chat/?(?:/__data.json)?$",
			"dest": "/api/ai/chat"
		},
		{
			"src": "^/api/ai/vision/?(?:/__data.json)?$",
			"dest": "/api/ai/vision"
		},
		{
			"src": "/.*",
			"dest": "/![-]/catchall"
		}
	],
	"overrides": {}
}